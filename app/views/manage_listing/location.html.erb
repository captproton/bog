    <div id="localized-fields">
    <div class="row-space-1">
      <%= f.label :street_address %>
      <%= f.text_field :street_address, placeholder: "e.g. 123 Main St.", autocomplete: "off"  %>
      
    </div>

    <div class="row-space-1">
      <%= f.label :apt_ste_bld %>
      <%= f.text_field :apt_ste_bld, placeholder: "e.g. Apt #7" %>
    </div>

    <div class="row-space-1">
      <%= f.label :city %>
      <%= f.text_field :city, placeholder: "e.g. San Francisco" %>
    </div>

    <div class="row-space-1">
      <%= f.label :state %>
      <%= f.text_field :state, placeholder: "e.g. CA" %>
    </div>

    <div class="row-space-1">
      <%= f.label :postal_code %>
      <%= f.text_field :postal_code, placeholder: "e.g. 94103"  %>
