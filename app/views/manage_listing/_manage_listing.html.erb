                              <i class="nav-icon icon icon-add icon-rausch"></i>
                            </div>
      
                          <i class="nav-icon icon icon-ok-alt icon-lima nav-icon-checked not-post-listed"></i>

                          <i class="icon icon-bolt icon-beach hide"></i>
                          <i class="dot dot-success hide"></i>
                          </div>
                        </div>
                      <% end %>
                  </li>

    
                  <li class="nav-item nav-photos pre-listed complete" data-track="photos">
