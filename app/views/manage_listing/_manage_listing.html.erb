              <div id="availability-dropdown">
            </div>
            <div id="js-publish-button"><div class="not-post-listed text-center">
                <div style="display: none;" class="animated text-lead text-muted steps-remaining js-steps-remaining anim-fade-out-scale-down"></div>
                <button style="opacity: 0; display: inline-block;" data-href="complete" class="animated btn btn-large btn-host btn-primary anim-flip-in-Y" id="js-list-space-button" data-track="list_space_button_left_nav">
                    List Space
                </button>
            </div>


        </div>
    </div>
</div>
<div class="manage-listing-content col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main"
 id="js-manage-listing-content">
    <%= yield %>
</div>
