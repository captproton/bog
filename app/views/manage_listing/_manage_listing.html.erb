                          <div class="col-9">Calendar</div>
                          <div class="col-3 text-center">
      
                            <div class="js-new-section-icon not-post-listed">
                              <i class="nav-icon icon icon-add icon-rausch"></i>
                            </div>
      

      
                          <i class="nav-icon icon icon-ok-alt icon-lima nav-icon-checked not-post-listed visible transition"></i>

                          <i class="icon icon-bolt icon-beach hide"></i>
                          <i class="dot dot-success hide"></i>
                          </div>
                        </div>
                      <% end %>
          
                  </li>

    
                  <li class="nav-item nav-pricing pre-listed complete" data-track="pricing">
