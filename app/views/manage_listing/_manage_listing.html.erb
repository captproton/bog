            </li>
                <%= content_tag :li, class: "subnav-text"  do -%>
                    <%= @space.title %>
                <% end %>
            <li>
                <%# link_to @space.title, "/" %>
                <%= link_to(room_path(@space), preview: true, title: "Preview your listing as it will appear when active.", target: "_blank", :'data-track' => 'preview', class: "subnav-item pull-right", id: "preview-btn") do %>
                    <i class="icon icon-eye"></i>
                    Preview
                <% end %>
            </li>
          </ul>
      </div>
    </div>

